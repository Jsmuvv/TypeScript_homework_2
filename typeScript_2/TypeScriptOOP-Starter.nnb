            "language": "typescript",
            "source": [
                "class Person1{\n    private _age: number;\n\n    get age():number{\n        return this._age\n    }\n\n    set age(value:number){\n        if (value > 0 && value <=110){\n            this._age = value\n        } else {\n            console.error(\"That is an invalid age\")\n        }\n    }\n}"
